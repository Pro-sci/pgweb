{
  "name": "pgweb",
  "description": "A web-based frontend to your Postgres database.",
  "website": "https://github.com/robanderton/pgweb",
  "repository": "https://github.com/robanderton/pgweb",
  "logo": "https://raw.githubusercontent.com/robanderton/pgweb/master/static/img/icon.png",
  "success_url": "/",
  "keywords": [
    "postgres",
    "database",
    "golang",
    "frontend"
  ],
  "env": {
    "AUTH_USER": {
      "description": "A username to log into pgweb.",
      "value": "admin"
    },
    "AUTH_PASS": {
      "description": "The login password. Please change this.",
      "generator": "secret"
    }
  },
  "formation": [
    {
      "quantity": 1,
      "size": "Private-S",
      "type": "web"
    }
  ]
}
